{
    "name": "baklysystems/laravel-dompdf",
    "description": "A DOMPDF Wrapper for Laravel with arabic support",
    "license": "MIT",
    "keywords": ["laravel", "dompdf", "pdf", "arabic"],
    "authors": [
        {
            "name": "Barry vd. Heuvel",
            "email": "barryvdh@gmail.com"
        },
  	{
            "name": "Hebatollah Moaz",
            "email": "hebatollah.moaz@gmail.com"
        }
    ],
    "require": {
        "php": ">=7",
        "illuminate/support": "5.5.x|5.6.x|5.7.x|5.8.x",
        "baklysystems/dompdf": "^0.8.5"
    },

    "autoload": {
        "psr-4": {
            "Barryvdh\\DomPDF\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.8-dev"
        },
        "laravel": {
            "providers": [
                "Barryvdh\\DomPDF\\ServiceProvider"
            ],
            "aliases": {
                "PDF": "Barryvdh\\DomPDF\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
